{#
/**
 * @file
 * Bootstrap Barrio's theme implementation for a branding block.
 *
 * Each branding element variable (logo, name, slogan) is only available if
 * enabled in the block configuration.
 *
 * Available variables:
 * - site_logo: Logo for site as defined in Appearance or theme settings.
 * - site_name: Name for site as defined in Site information settings.
 * - site_slogan: Slogan for site as defined in Site information settings.
 */
#}
{# {% block content %} #}
{% if content.novartis_dolphin_header_logo_path %}
  {% set classes = 'pre-logo' %}
{% endif %}
  {% if site_logo %}
    <a href="{{ path('<front>') }}" title="{{ site_logo_title }}" rel="{{ site_logo_title }}" class="site-logo" onclick="void(0)">
      <object class="logo pre-interaction-logo {{classes}}" data="{{ site_logo }}">
        <img src="{{ site_logo }}" alt="{{ site_logo_alt }}"/>
      </object>
      {% if content.novartis_dolphin_header_logo_path %}
        <object class="logo post-interaction-logo" data="{{ content.novartis_dolphin_header_logo_path }}"/>
          <img src="{{ content.novartis_dolphin_header_logo_path }}" alt="{{ site_logo_alt }}"/>
        </object>
      {% endif %}
    </a>
  {% endif %}
{# {% if site_name %}
    <div class="site-name">
      <a href="{{ path('<front>') }}" title="{{ 'Home'|t }}" rel="home">{{ site_name }}</a>
    </div>
  {% endif %}
  {% if site_slogan %}
    <div class="site-slogan">{{ site_slogan }}</div>
  {% endif %}
{% endblock %} #}
