{
  "attributes": {
    "Attribute()": {
      "class": ["form-element--class"]
    }
  },
  "type": "text",
  "name": "form-name",
  "label": {
    "join()": [{
      "include()": {
        "pattern": "atoms-label",
        "with": {
          "title": "",
          "title_display": "before",
          "attributes": {
            "Attribute()": {
              "class": ["form__label--class"]
            }
          }
        }
      }
    }]
  },
  "children": {
    "join()": [{
      "include()": {
        "pattern": "atoms-select",
        "with": {
          "options": [{
            "type": "option",
            "value": "most recent",
            "label": "Most recent"
          },{
            "type": "option",
            "value": "most relevant",
            "label": "Most relevant"
          }],
          "children": null
        }
      }
    }]
  },
  "label_display": "before",
  "description": {
    "content": "",
    "attributes": {
      "Attribute()": {
        "class": ["form-element__description--class"]
      }
    }
  },
  "description_display":"after",
  "disabled": false,
  "title_display": "before"
}
