{
  "attributes": {
    "Attribute()": {
      "class": ["views-exposed-form"]
    }
  },
  "content": {
    "join()": [{
      "include()": {
        "pattern": "molecules-form",
        "with": {
          "attributes": {
            "Attribute()": {
              "id": "views-exposed-form-acquia-search-page"
            }
          },
          "children": {
            "join()": [{
              "include()": {
                "pattern": "molecules-views-exposed-form",
                "with": {
                  "form": {
                    "join()": [{
                      "include()": {
                        "pattern": "molecules-form-elements",
                        "with": {
                          "attributes": {
                            "Attribute()": {
                              "class": ["form-item-search"]
                            }
                          },
                          "type": "text",
                          "name": "form-name",
                          "label": {
                            "join()": [{
                              "include()": {
                                "pattern": "atoms-label",
                                "with": {
                                  "title": "",
                                  "title_display": "before",
                                  "attributes": {
                                    "Attribute()": {
                                      "class": ["form__label--class"]
                                    }
                                  }
                                }
                              }
                            }]
                          },
                          "children": {
                            "join()": [{
                              "include()": {
                                "pattern": "atoms-textfield",
                                "with": {
                                  "attributes": {
                                    "Attribute()": {
                                      "class": ["input"],
                                      "id": "edit-search",
                                      "type": "input",
                                      "required": "required",
                                      "placeholder": "'Search Term'"
                                    }
                                  },
                                  "children": null
                                }
                              }
                            }]
                          },
                          "label_display": "before",
                          "description": {
                            "content": "",
                            "attributes": {
                              "Attribute()": {
                                "class": ["form-element__description--class"]
                              }
                            }
                          },
                          "description_display":"after",
                          "disabled": false,
                          "title_display": "before"
                        }
                      }
                    },{
                      "include()": {
                        "pattern": "molecules-form-elements",
                        "with": {
                          "attributes": {
                            "Attribute()": {
                              "class": ["js-form-wrapper", "form-actions"]
                            }
                          },
                          "children": {
                            "join()": [{
                              "include()": {
                                "pattern": "atoms-textfield",
                                "with": {
                                  "attributes": {
                                    "Attribute()": {
                                      "id": "edit-submit-acquia-search",
                                      "type": "submit",
                                      "required": "required"
                                    }
                                  },
                                  "children": null
                                }
                              }
                            }]
                          }
                        }
                      }
                    }]
                  }
                }
              }
            }]
          }
        }
      }
    }]
  }
}